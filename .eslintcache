alue => prevValue + 3);\n\n  const carsList = cards.slice(0, numberOfCards).map(card => {\n    return <NewsCard\n      key={card.id}\n      className={'news__item'}\n      card={card}\n      isLogin={isLogin}\n    />\n   });\n\n  return (\n    <>\n      { title && <h2\n        className={'news__title'}>{title}</h2>}\n\n      <ul className={newsClassName}>\n        {carsList}\n      </ul>\n\n      { (button && isLastCard) && <Button\n        text={'Показать еще'}\n        className={'button_type_show-card'}\n        onClick={showMoreCards}\n      /> }\n    </>\n  );\n}\n","/Users/german/dev/yandex/news-explorer-frontend/src/components/NewsCard/NewsCard.js",["179","180","181"],"import React from 'react';\nimport './NewsCard.css';\nimport {DeleteIcon} from \"../Icons/DeleteIcon\";\nimport {Button} from \"../Button/Button\";\nimport {useRouteMatch} from \"react-router-dom\";\nimport {Checkbox} from \"../Checkbox/Checkbox\";\nimport {ExternalLink} from \"../Link/ExternalLink\";\n\nexport const NewsCard = ({card, isLogin}) => {\n\n  const {\n    keyword,\n    title,\n    text,\n    date,\n    source,\n    link,\n    image,\n    saved,\n    set,\n    id,\n  } = card;\n\n  const {path} = useRouteMatch();\n\n  return (\n    <li\n      key={id}\n      className={'news__item card'}\n    >\n      <figure className={'card__wrapper'}>\n          {\n            path !== '/'\n            ? <Button className={'card__button'}>\n                <DeleteIcon/>\n                <p className={'card__popup card__popup_prompt'}>\n                  Убрать из сохранённых\n                </p>\n              </Button>\n            : <Checkbox\n              className={'card__button'}\n              isLogin={isLogin}\n            />\n          }\n\n        {\n          keyword\n          && (<p className={'card__popup card__popup_keyword'}>{keyword}</p>)\n        }\n\n        <ExternalLink\n          className={'card__link'}\n          href={'https://medium.com/'}\n          target={'_blank'}\n        >\n       <img\n         className={'card__img'}\n         alt={title}\n         src={image}\n       />\n        <figcaption className={'card__caption'}>\n          <data className={'card__date'}>{date}</data>\n          <h3 className={'card__title'}>{title}</h3>\n          <p className={'card__text'}>{text}</p>\n          <p className={'card__source'}>{source}</p>\n        </figcaption>\n        </ExternalLink>\n      </figure>\n    </li>\n  );\n}\n","/Users/german/dev/yandex/news-explorer-frontend/src/components/Icons/Bookmark.js",[],"/Users/german/dev/yandex/news-explorer-frontend/src/components/Icons/NotFound.js",[],"/Users/german/dev/yandex/news-explorer-frontend/src/components/Preloader/Preloader.js",[],"/Users/german/dev/yandex/news-explorer-frontend/src/components/About/About.js",[],"/Users/german/dev/yandex/news-explorer-frontend/src/components/Footer/Footer.js",["182","183","184"],"import React from 'react';\nimport './Footer.css';\nimport {Navigation} from \"../Navigation/Navigation\";\nimport {NavLink} from \"react-router-dom\";\nimport {GitHubIcon} from \"../Icons/GitHubIcon\";\nimport {FacebookIcon} from \"../Icons/FacebookIcon\";\n\nexport const Footer = () => {\n\n  return (\n      <footer className=\"footer wrapper__content\">\n\n        <p className=\"footer__copyright\">&copy; 2020 Supersite, Powered by News API</p>\n\n        <div className={'footer__nav'}>\n          <Navigation\n            classNameList={'nav__list_footer'}\n          >\n            <NavLink to={'./'} className=\"link footer__link\">Главная</NavLink>\n            <a href=\"#\" className=\"link footer__link\">Яндекс.Практикум</a>\n          </Navigation>\n\n          <Navigation\n            classNameList={'nav__list_footer footer__nav_social'}\n          >\n            <a href=\"#\" className=\"link footer__link\">\n              <GitHubIcon/>\n            </a>\n            <a href=\"#\" className=\"link footer__link footer__link_radius\">\n              <FacebookIcon\