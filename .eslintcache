} = card;\n\n  const loggedIn = Object.keys(currentUser).length !== 0;\n  const {path} = useRouteMatch();\n\n  return (\n    <li\n      key={id}\n      className={'news__item card'}\n    >\n      <figure className={'card__wrapper'}>\n          {\n            path !== '/'\n            ? <Button className={'card__button'}>\n                <DeleteIcon/>\n                <p className={'card__popup card__popup_prompt'}>\n                  Убрать из сохранённых\n                </p>\n              </Button>\n            : <Checkbox\n              className={'card__button'}\n              loggedIn={loggedIn}\n            />\n          }\n\n        {\n          keyword\n          && (<p className={'card__popup card__popup_keyword'}>{keyword}</p>)\n        }\n\n        <ExternalLink\n          className={'card__link'}\n          href={'https://medium.com/'}\n          target={'_blank'}\n        >\n       <img\n         className={'card__img'}\n         alt={title}\n         src={image}\n       />\n        <figcaption className={'card__caption'}>\n          <data className={'card__date'}>{date}</data>\n          <h3 className={'card__title'}>{title}</h3>\n          <p className={'card__text'}>{text}</p>\n          <p className={'card__source'}>{source}</p>\n        </figcaption>\n        </ExternalLink>\n      </figure>\n    </li>\n  );\n}\n","/Users/german/dev/yandex/news-explorer-frontend/src/components/Icons/Bookmark.js",[],"/Users/german/dev/yandex/news-explorer-frontend/src/components/Icons/NotFound.js",[],"/Users/german/dev/yandex/news-explorer-frontend/src/components/Preloader/Preloader.js",[],"/Users/german/dev/yandex/news-explorer-frontend/src/components/About/About.js",[],"/Users/german/dev/yandex/news-explorer-frontend/src/components/Footer/Footer.js",["181","182","183"],"import React from 'react';\nimport './Footer.css';\nimport {Navigation} from \"../Navigation/Navigation\";\nimport {NavLink} from \"react-router-dom\";\nimport {GitHubIcon} from \"../Icons/GitHubIcon\";\nimport {FacebookIcon} from \"../Icons/FacebookIcon\";\n\nexport const Footer = () => {\n\n  return (\n      <footer className=\"footer wrapper__content\">\n\n        <p className=\"footer__copyright\">&copy; 2020 Supersite, Powered by News API</p>\n\n        <div className={'footer__nav'}>\n          <Navigation\n            classNameList={'nav__list_footer'}\n          >\n            <NavLink to={'./'} className=\"link footer__link\">Главная</NavLink>\n            <a href=\"#\" className=\"link footer__link\">Яндекс.Практикум</a>\n          </Navigation>\n\n          <Navigation\n            classNameList={'nav__list_footer footer__nav_social'}\n          >\n            <a href=\"#\" className=\"link footer__link\">\n              <GitHubIcon/>\n            </a>\n            <a href=\"#\" className=\"link footer__link footer__link_radius\">\n              <FacebookIcon\n                radius={true}\n              />\n            </a>\n          </Navigation>\n        </div>\n      </footer>\n  );\n}\n","/Users/german/dev/yandex/news-explorer-frontend/src/components/Link/Link.js",[],"/Users/german/dev/yandex/news-explorer-frontend/src/pages/SavedNewsPage.js",[],"/Users/german/dev/yandex/news-explorer-frontend/src/components/Icons/DeleteIcon.js",[],"/Users/german/dev/yandex/news-explorer-frontend/src/pages/MainPage.js",[],"/Users/german/dev/yandex/news-explorer-frontend/src/components/SavedNews/SavedNews.js",[],"/Users/german/dev/yandex/news-explorer-frontend/src/components/Icons/MenuIcon.js",[],"/Users/german/dev/yandex/news-explorer-frontend/src/components/Icons/GitHubIcon.js",[],"/Users/german/dev/yandex/news-explorer-frontend/src/components/Icons/FacebookIcon.js",[],"/Users/german/dev/yandex/news-explorer-frontend/src/components/Checkbox/Checkbox.js",[],"/Users/german/dev/yandex/news-explorer-frontend/src/contexts/currentUserContext.js",[],"/Users/german/dev/yandex/news-explorer-frontend/src/contexts/CurrentUserContext.js",[],"/Users/german/dev/yandex/n